{
  "name": "Candy - Dark",
  "type": "dark",
  "colors": {
    "editor.background": "#2C253F",
    "editor.foreground": "#FFFFFF",
    "editor.lineHighlightBackground": "#3A2F50",
    "editor.selectionBackground": "#4D3E66",
    "editorCursor.foreground": "#FF659C",
    "sideBar.background": "#2C253F",
    "sideBarSectionHeader.background": "#2C253F",
    "activityBar.background": "#2C253F",
    "titleBar.activeBackground": "#2C253F",
    "titleBar.inactiveBackground": "#2C253F",
    "statusBar.background": "#2C253F",
    "panel.background": "#2C253F",
    "editorGroupHeader.tabsBackground": "#2C253F",
    "tab.activeBackground": "#3A2F50",
    "tab.inactiveBackground": "#2C253F",
    "breadcrumb.background": "#2C253F",
    "peekViewEditor.background": "#2C253F",
    "peekViewResult.background": "#2C253F",
    "peekViewTitle.background": "#2C253F",
    "input.background": "#3A2F50",
    "dropdown.background": "#3A2F50",
    "menu.background": "#3A2F50",
    "menubar.selectionBackground": "#4D3E66",
    "notificationCenterHeader.background": "#2C253F",
    "notifications.background": "#3A2F50",
    "editorWidget.background": "#2C253F",
    "settings.headerBackground": "#2C253F",
    "settings.rowBackground": "#3A2F50",
    "terminal.background": "#2C253F",
    "debugToolBar.background": "#2C253F",
    "welcomePage.background": "#2C253F"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {"foreground": "#807796"}
    },
    {
      "scope": ["string", "string.quoted", "string.template", "punctuation.definition.string"],
      "settings": {"foreground": "#DFD473"}
    },
    {
      "scope": ["constant.numeric", "constant.language", "constant.character"],
      "settings": {"foreground": "#7A7FFD"}
    },
    {
      "scope": ["keyword", "keyword.control", "keyword.operator", "storage.type", "storage.modifier", "keyword.other", "meta.export"],
      "settings": {"foreground": "#FF659C"}
    },
    {
      "scope": ["variable", "variable.other.readwrite", "variable.parameter", "entity.name.variable", "support.variable"],
      "settings": {"foreground": "#73DFA5"}
    },
    {
      "scope": ["variable.language", "variable.language.this", "variable.language.super"],
      "settings": {"foreground": "#FFFFFF"}
    },
    {
      "scope": ["constant.other", "support.constant"],
      "settings": {"foreground": "#E08569"}
    },
    {
      "scope": ["entity.other.attribute-name", "entity.other.inherited-class"],
      "settings": {"foreground": "#73DFA5"}
    },
    {
      "scope": ["entity.name.function", "support.function", "meta.function"],
      "settings": {"foreground": "#73DFA5"}
    },
    {
      "scope": ["variable.other.property", "support.type.property-name", "meta.object-literal.key"],
      "settings": {"foreground": "#1AC8FF"}
    },
    {
      "scope": ["meta", "meta.structure", "meta.type", "meta.brace", "meta.block"],
      "settings": {"foreground": "#F1ECFE"}
    },
    {
      "scope": ["entity.name.tag", "meta.tag", "punctuation.definition.tag"],
      "settings": {"foreground": "#FF659C"}
    },
    {
      "scope": ["entity.name.type", "support.type", "storage.type.primitive", "meta.type.name"],
      "settings": {"foreground": "#1AC8FF"}
    }
  ]
} 