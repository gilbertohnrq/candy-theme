{
  "name": "Candy - Dark",
  "type": "dark",
  "colors": {
    "editor.background": "#2C253F",
    "editor.foreground": "#FFFFFF",
    "editor.lineHighlightBackground": "#3A2F50",
    "editor.selectionBackground": "#4D3E66",
    "editorCursor.foreground": "#FF659C",
    "sideBar.background": "#2C253F",
    "sideBarSectionHeader.background": "#2C253F",
    "activityBar.background": "#2C253F",
    "titleBar.activeBackground": "#2C253F",
    "titleBar.inactiveBackground": "#2C253F",
    "statusBar.background": "#2C253F",
    "panel.background": "#2C253F",
    "editorGroupHeader.tabsBackground": "#2C253F",
    "tab.activeBackground": "#3A2F50",
    "tab.inactiveBackground": "#2C253F",
    "breadcrumb.background": "#2C253F",
    "peekViewEditor.background": "#2C253F",
    "peekViewResult.background": "#2C253F",
    "peekViewTitle.background": "#2C253F",
    "input.background": "#3A2F50",
    "dropdown.background": "#3A2F50",
    "menu.background": "#3A2F50",
    "menubar.selectionBackground": "#4D3E66",
    "notificationCenterHeader.background": "#2C253F",
    "notifications.background": "#3A2F50",
    "editorWidget.background": "#2C253F",
    "settings.headerBackground": "#2C253F",
    "settings.rowBackground": "#3A2F50",
    "terminal.background": "#2C253F",
    "debugToolBar.background": "#2C253F",
    "welcomePage.background": "#2C253F"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {"foreground": "#807796"}
    },
    {
      "scope": ["string", "string.quoted", "string.template", "punctuation.definition.string", "string.quoted.single", "string.quoted.double"],
      "settings": {"foreground": "#DFD473"}
    },
    {
      "scope": ["constant.numeric", "constant.language", "constant.character", "constant.language.boolean"],
      "settings": {"foreground": "#7A7FFD"}
    },
    {
      "scope": ["keyword", "keyword.control", "keyword.declaration", "keyword.other", "storage.type", "storage.modifier", "keyword.import", "keyword.control.import", "keyword.control.export", "keyword.control.from"],
      "settings": {"foreground": "#FF659C"}
    },
    {
      "scope": ["keyword.operator", "punctuation.operator", "keyword.operator.arithmetic", "keyword.operator.assignment", "keyword.operator.comparison", "keyword.operator.logical"],
      "settings": {"foreground": "#FF659C"}
    },
    {
      "scope": ["entity.name.class", "entity.name.type.class", "support.class"],
      "settings": {"foreground": "#1AC8FF"}
    },
    {
      "scope": ["entity.name.type", "support.type", "storage.type.primitive", "entity.name.type.interface", "entity.name.type.enum"],
      "settings": {"foreground": "#1AC8FF"}
    },
    {
      "scope": ["entity.name.function", "support.function", "meta.function-call", "entity.name.function.constructor"],
      "settings": {"foreground": "#73DFA5"}
    },
    {
      "scope": ["variable", "variable.other.readwrite", "variable.parameter", "entity.name.variable", "variable.other.local"],
      "settings": {"foreground": "#73DFA5"}
    },
    {
      "scope": ["variable.language", "variable.language.this", "variable.language.super", "constant.language.null", "constant.language.undefined"],
      "settings": {"foreground": "#FFFFFF"}
    },
    {
      "scope": ["variable.other.constant", "variable.other.enummember", "constant.other"],
      "settings": {"foreground": "#E08569"}
    },
    {
      "scope": ["variable.other.property", "support.type.property-name", "meta.object-literal.key"],
      "settings": {"foreground": "#1AC8FF"}
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {"foreground": "#73DFA5"}
    },
    {
      "scope": ["entity.name.tag", "meta.tag", "punctuation.definition.tag"],
      "settings": {"foreground": "#FF659C"}
    },
    {
      "scope": ["punctuation.separator", "punctuation.terminator", "punctuation.delimiter", "punctuation.accessor", "punctuation.section"],
      "settings": {"foreground": "#FFFFFF"}
    },
    {
      "scope": ["meta.brace", "punctuation.definition.block", "punctuation.definition.array", "punctuation.definition.parameters"],
      "settings": {"foreground": "#F1ECFE"}
    },
    {
      "scope": ["entity.name.namespace", "entity.name.module"],
      "settings": {"foreground": "#73DFA5"}
    }
  ]
} 